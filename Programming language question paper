def STUDENT_INFO():
       NAME_STUDENT=str(input("ENTER THE NAME"))
       CLASS_STUDENT=str(input("ENTER CLASS"))
       MAIL_ADDRESS=str(input("ENTER THE MAIL ADDRESS"))
       DATE=str(input("ENTER THE DATE"))
       print("PAPER WILL START IN 3 SEC..")
       time.sleep(3)
       print(question_paper())
       ALL_INFO = ("STUDENT NAME-{}\nSTUDENT CLASS-{}\nMAIL ADDRESS-{}\nDATE OF EXAM-{}\n" .format(NAME_STUDENT,CLASS_STUDENT,MAIL_ADDRESS,DATE))
       print(ALL_INFO)
       print("CORRECT ANSWER",sum(CORRECT_QUESTION))
       print("WRONG QUESTION",sum(WRONG_QUESTION))
STUDENT_INFO()
